{
  "name": "generator-toptive-python",
  "version": "1.0.2",
  "description": "Toptive yeoman generator for python projects",
  "homepage": "https://github.com/toptive/generator-toptive-python#readme",
  "author": "Franco Riberi <fgriberi@gmail.com>",
  "files": [
    "generators"
  ],
  "main": "index.js",
  "keywords": [
    "yeoman-generator",
    "python",
    "python-library",
    "cli",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^29.3.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "scripts": {
    "test": "jest",
    "pretest": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toptive/generator-toptive-python.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toptive/generator-toptive-python/issues"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  }
}
